{
	"name": "rpi-lighthub",
	"version": "0.1.0",
	"description": "Simple LED blink in node.js with resin.io on pi3",
	"main": "main.js",
	"scripts": {
		"start": "gulp --env prod && node --harmony ./dist/start --env prod -d 'front-end' -d 'autoupdate'",
		"stage": "gulp --env stage && node --harmony ./dist/start --env stage",
		"dev": "gulp --env dev && node --harmony ./dist/start --env dev",
		"watch": "gulp --env dev && (gulp watch --env dev &) && node --harmony ./dist/start --env dev -d 'front-end'",
		"autoupdate": "gulp --env dev && node --harmony ./dist/start --env prod -d 'front-end' -d 'autoupdate'"
	},
	"browserify": {
		"transform": [
			[
				"babelify",
				{
					"presets": [
						"es2015"
					]
				}
			]
		]
	},
	"browser": {
		"TweenMax": "./node_modules/gsap/src/minified/TweenMax.min.js",
		"jquery.gsap": "./node_modules/gsap/src/minified/jquery.gsap.min.js",
		"jquery.browser": "./node_modules/jquery.browser/dist/jquery.browser.js",
		"howler": "./node_modules/howler/howler.js"
	},
	"author": "Patrik Gunderson",
	"license": "MIT",
	"repository": "https://github.com/gunderson/rpi3-lighthub-resin.git",
	"dependencies": {
		"art-kit": "github:gunderson/art-kit",
		"babel": "^6.5.2",
